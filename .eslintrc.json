{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["next/core-web-vitals", "eslint:recommended", "plugin:react/recommended", "next"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "no-unused-vars": "warn",
        "@next/next/no-img-element": "off",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off",
        "@next/next/no-page-custom-font": "off"
    }
}
